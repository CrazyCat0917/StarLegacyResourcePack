{
    "multipart": [
        {
            "when": { "down": true, "east": true, "north": true, "south": true, "up": true, "west": true },
            "apply": { "model": "block/custom/ores/oriomium_block"}
	},
	{
            "when": { "down": false, "east": true, "north": true, "south": true, "up": false, "west": true },
            "apply": { "model": "block/custom/ores/uranium_block"}
        }
    ]
}
